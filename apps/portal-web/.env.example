# Supabase Configuration
# Copy this file to .env.local and fill in your actual values

# Supabase URL - Get this from your Supabase project dashboard
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
SUPABASE_URL=your_supabase_url_here

# Supabase Anonymous Key - Get this from your Supabase project dashboard
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Service Role Key - Get this from your Supabase project dashboard (keep this secret!)
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Supabase JWT Secret - Get this from your Supabase project dashboard
SUPABASE_JWT_SECRET=your_supabase_jwt_secret_here

# Strava API Configuration
# Get these from your Strava API application: https://developers.strava.com/
NEXT_PUBLIC_STRAVA_CLIENT_ID=your_strava_client_id_here
STRAVA_CLIENT_ID=your_strava_client_id_here
STRAVA_CLIENT_SECRET=your_strava_client_secret_here
STRAVA_WEBHOOK_VERIFY_TOKEN=your_webhook_verify_token_here
